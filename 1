#!groovy
// Check ub1 properties
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                sh 'ssh -i /home/orest/Завантаження/Kluch_3.pem ubuntu@ec2-3-88-8-11.compute-1.amazonaws.com \'hostname\''
            }
        }
        stage("Second step") {
            steps {
                sh 'ssh -i /home/orest/Завантаження/Kluch_3.pem ubuntu@ec2-3-88-8-11.compute-1.amazonaws.com\'uptime\''
            }
        }
    }
}
